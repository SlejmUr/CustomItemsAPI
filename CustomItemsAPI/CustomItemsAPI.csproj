<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
	  <AssemblyName>CustomItemsAPI</AssemblyName>
	  <RootNamespace>CustomItemsAPI</RootNamespace>
	  <Copyright>SlejmUr</Copyright>
	  <GenerateDocumentationFile>True</GenerateDocumentationFile>
	  <FileVersion>$(Version)</FileVersion>
	  <AssemblyVersion>$(Version)</AssemblyVersion>
	  <Version>0.0.3.0</Version>
	  <GeneratePackageOnBuild>False</GeneratePackageOnBuild>
	  <Authors>SlejmUr</Authors>
	  <Company>SlejmUr</Company>
	  <PackageReadmeFile>README.md</PackageReadmeFile>
	  <RepositoryUrl>https://github.com/SlejmUr/CustomItemsAPI</RepositoryUrl>
	  <PackageProjectUrl>https://github.com/SlejmUr/CustomItemsAPI</PackageProjectUrl>
	  <Description>LabAPI CustomItems implementation.</Description>
	  <PackageLicenseFile>LICENSE.txt</PackageLicenseFile>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\LICENSE.txt">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
    <None Include="..\README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
